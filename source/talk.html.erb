---
layout: layout
---

<% content_for(:head) do %>
  <% url = "/talks/#{talk.slug}" %>
  <link rel="canonical" href="<%= url %>">
<% end %>

<h1><%= talk.title %></h1>

<% talk.authors.each do |a| %>
  <%= a.name %> <%= a.twitter %>
  <img src="<%= profile_image(a) %>">
<% end %>

<%= talk.conference %>

<%= embedded_video(talk) %>

<%= talk.description %>

about the presenters:
<% talk.authors.each do |a| %>
  <%= a.bio %>
<% end %>
